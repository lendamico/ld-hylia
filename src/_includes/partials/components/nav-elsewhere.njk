
  <nav class="nav">
    <ul class="[ nav__list ] [ box-flex align-center md:space-before ]">
      {% for item in elswehere.items %}
        {% set relAttribute = '' %}
        {% set currentAttribute = '' %}

        {% if page.url == item.url %}
          {% set currentAttribute = ' aria-current="page"' %}
        {% endif %}

        <li class="nav__item">
          <a href="{{ item.url }}" rel="me" {{ currentAttribute | safe }}>{{ item.link }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
